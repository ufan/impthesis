	keywords = {Nuclear Experiment, Physics - Instrumentation and Detectors},
	file = {arXiv\:1508.05618 PDF:/Users/yong/Library/Application Support/Zotero/Profiles/9z4ucssq.default/zotero/storage/WW3Q3AVK/Tang et al. - 2015 - A large area plastic scintillation detector with 4.pdf:application/pdf;arXiv.org Snapshot:/Users/yong/Library/Application Support/Zotero/Profiles/9z4ucssq.default/zotero/storage/DJ2T3JBR/1508.html:text/html}
}

