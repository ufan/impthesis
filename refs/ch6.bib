	author = {Tang, Shu-wen and Yu, Yu-hong and Zhou, Yong and Sun, Zhi-yu and Zhang, Xue-heng and Wang, Shi-tao and Yue, Ke and Liu, Long-xiang and Fang, Fang and Yan, Duo and Sun, Yu and Wang, Zhao-min},
	month = aug,
